    "@babel/preset-env": "^7.7.7",
    "@babel/preset-react": "^7.7.4",
    "@babel/preset-stage-0": "^7.0.0",
    "axios": "^0.19.1",
    "babel-loader": "^8.0.6",
    "compression": "^1.7.4",
    "concurrently": "^5.0.2",
    "express": "^4.17.1",
    "express-http-proxy": "^1.6.0",
    "lodash": "^4.17.15",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "react-redux": "^7.1.3",
